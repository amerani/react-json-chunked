{
  "name": "react-json-chunked",
  "version": "0.3.4",
  "description": "A React component for streaming JSON using chunked Transfer-Encoding",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "vite build",
    "dev": "npm-run-all --parallel --print-label watch:**",
    "watch:src": "vite build --watch",
    "watch:dist": "nodemon -w dist --exec 'vite'",
    "test": "vitest",
    "test:run": "vitest run",
    "version": "changeset && changeset version",
    "release": "changeset publish"
  },
  "author": "Alek Merani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amerani/react-json-chunked.git"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "glob": "^11.0.3",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "unplugin-dts": "^1.0.0-beta.6",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
